<?xml version="1.0" encoding="UTF-8"?>
<dynaForm
  name="cases_Resume"
  type="xmlform"
  width="500px"
  labelWidth="100px"
>

<TITLE1 type="title">
  <en>Case Resume</en>
</TITLE1>

<TITLE type="text" mode="view">
  <en>Case Title</en>
</TITLE>

<CREATOR type="text" mode="view">
  <en>Creator</en>
</CREATOR>

<CREATE_DATE type="text" mode="view">
  <en>Create Date</en>
</CREATE_DATE>

<UPDATE_DATE type="text" mode="view">
  <en>Last Update</en>
</UPDATE_DATE>

<STATUS type="text" mode="view">
  <en>Case Status</en>
</STATUS>

<DEL_THREAD_STATUS type="dropdown" mode="view">
  <en>Current Task Status
    <option name="OPEN">Active</option>
    <option name="CLOSE">Finished</option>
    <option name="PAUSED">Paused</option>
  </en>
</DEL_THREAD_STATUS>

</dynaForm>