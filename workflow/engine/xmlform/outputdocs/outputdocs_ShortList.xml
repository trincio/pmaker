<?xml version="1.0" encoding="UTF-8"?>
<dynaForm
  sqlConnection=""
  menu="outputdocs/outputdocs_Options"
  width="100%"
  rowsPerPage="4"
>

<OUT_DOC_UID type="hidden" showInTable="0"/>

<OUT_DOC_TITLE type="text" colWidth="350" titleAlign="left" align="left">
  <en>Output document</en>
  <es>Documento requerido</es>
</OUT_DOC_TITLE>

<EDIT type="link" colWidth="40" value="@G::LoadTranslation(ID_EDIT)" link="#" onclick="outputdocsEdit(@QOUT_DOC_UID);return false;"/>

<DELETE type="link" colWidth="40" value="@G::LoadTranslation(ID_DELETE)" link="#" onclick="outputdocsDelete(@QOUT_DOC_UID);return false;"/>

<!-- FILTER FORM -->
<SEARCH type="button" onclick="pagedTableFilter(this.form);" showInTable="0">
  <en>Apply Filter</en>
  <es>Filtrar</es>
</SEARCH>

<PAGED_TABLE_ID type="private" showInTable="0"/>

<JSFILTER type="javascript" replaceTags="1" showInTable="0">
function pagedTableFilter(form) {
  @#PAGED_TABLE_ID.doFilter(form);
}
</JSFILTER>

</dynaForm>